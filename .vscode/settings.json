{
    "liveSassCompile.settings.formats":[
       {
           "format": "expanded",
           "extensionName": ".css",
           "savePath": "/dev//css"
       }
    //    {
    //        "extensionName": ".css",
    //        "format": "compressed",
    //        "savePath": "/deploy/css"
    //    }
   ],
   "liveSassCompile.settings.excludeList": [
      "**/node_modules/**",
      ".vscode/**"
   ],
   "liveSassCompile.settings.generateMap": false,
   "liveSassCompile.settings.autoprefix": [
       "> 1%",
       "last 2 versions"
   ],

   "emeraldwalk.runonsave": {
    "commands": [
        {
            "match": "src\\\\js\\\\*",
            "isAsync": false,
            "cmd": "webpack"
        }
    ]}
}
